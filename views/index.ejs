

<!DOCTYPE html>
<html>
<head>
    <title>Home Page</title>
    <link rel="stylesheet" href="style.css">
      <link
    href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
    rel="stylesheet"
  >
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<body>
    <form id="DigiForm" class="DigiDetails">
             <img id="photo" src="">
             <h3 id="name"></h3>
             <h3 id="rank"></h3>
             <h3 id="level"></h3>
             <h3 id="attribute"></h3>
             <div class="horizontal">
             <h6 id="hp"></h6>
             <h6 id="attack"></h6>
             <h6 id="defense"></h6>
             </div>
             <br>
             <div class="horizontal">
              <button class="button submit-btn">accept</button>
              <button class="button cancel-btn">cancel</button>
             </div>

        </form>

         <form id="DigiPhoto" class="DigiDetails">
             <img id="photopage" src="">
             <div class="horizontal"></div>
              <button class="button close-btn">close</button>
             </div>

        </form>
    
    <div class="content d-flex flex-column flex-column-fluid" >
		
<div class="container" >
<div class="text-end">
  <button id="addDigimon" type="button" class="btn btn-primary">
    <i class="fa fa-plus"></i> Add Digimon
  </button>
</div>
        

<table id="DigiList">
<thead>
  <tr>
    <th><span class="th-text">Photo</span></th>
    <th><span class="th-text">Name</span></th>
    <th><span class="th-text">Rank</span></th>
    <th><span class="th-text">Level</span></th>
    <th><span class="th-text">Attribute</span></th>
    <th><span class="th-textSpec">HP/AT/DE</span></th>
    <th><span class="th-text">level</span></th>
    <th><span class="th-text">exp</span></th>
    <th><span class="th-text">Actions</span></th>
  </tr>
</thead>
<tbody><%- include('digimonTable') %></tbody>
</table>
</div>
</div>

</div>

</body>
<script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
<script src="/script.js"></script>
</html>






